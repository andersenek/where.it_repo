<div class="home">
<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>
<%= link_to "View All Looks", posts_path %>
<%= link_to "Add Your Look", new_post_path %>

<%= form_tag(posts_path, :method => "get", id: "search-form") do %>
<%= text_field_tag :search, params[:search], placeholder: "Search Looks" %>
<%= submit_tag "Search" %>
<% end %>
</div>

<script>
$('body').addClass('home-page');
</script>
